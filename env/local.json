{
  "logFile": "./logs/app.log",
  "mongo": {
    "replicas": [
      "localhost:27017"
    ],
    "database": "expressapi",
    "username": "admin",
    "password": "root",
    "options": {
      "socketTimeoutMS": 0,
      "autoReconnect": true,
      "keepAlive": true,
      "reconnectInterval": 1000,
      "bufferMaxEntries": 0,
      "readPreference": "secondaryPreferred",
      "poolSize": 8,
      "w": "majority",
      "connectTimeoutMS": 20000,
      "useNewUrlParser": true
    }
  },
  "redis": {
    "port": 6379,
    "host": "127.0.0.1",
    "db": 7,
    "options": {
      "retry_max_delay": 5000
    }
  },
  "channel": "orchestrator-channel",
  "writerMsg": "writerMsg",
  "adminKey": "test",
  "intHash": "dbwriter:instances",
  "hbHash": "dbwriter:heartbeat",
  "heartbeat": {
    "interval": 3000
  }
}